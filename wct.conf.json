{
    "suites": ["node_modules/@furo/fbp/test/*_test.html"],
    "npm": true,
    "moduleResolution": "node",
    "verbose": true,
    "plugins": {
        "istanbub": {
            "dir": "./coverage",
            "reporters": ["text-summary", "lcov"],
            "include": [
                "node_modules/@furo/fbp/*.js"
            ],
            "exclude": []
        },
        "local": {
            "browsers": ["chrome"]
        }
    }

}
