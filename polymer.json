{
  "entrypoint": "index.html",
  "shell": "_doc/main-app.js",
  "fragments": [
    "_doc/*.html"
  ],
  "sources": [
    "_assets/**/*",
    "custom/**/*",
    "_doc/**/*",
    "coverage/**/*",
    "node_modules/@furo/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "preset": "es6-unbundled"
    }
  ],
  "moduleResolution": "node",
  "npm": true,
  "lint": {
    "rules": [
      "polymer-3"
    ]
  }
}
